{
  "name": "duster-brews-dashboard",
  "version": "1.0.0",
  "description": "A web-app designed to display data from DIY I/O devices for beer monitoring and production.",
  "main": "server.js",
  "scripts": {
    "nodemon-watch": "nodemon ./server.js -e *",
    "start": "NODE_ENV=development & npm-run-all --parallel nodemon-watch",
    "seed": "node ./seed.js",
    "dev": "npm run start",
    "serve": "npm install & NODE_ENV=production & node ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrian-ortega/duster-brews-dashboard.git"
  },
  "keywords": [
    "Duster",
    "Brews",
    "Beer",
    "Dashboard"
  ],
  "contributors": [
    {
      "name": "Adrian Ortega",
      "email": "hello@adrianortega.dev",
      "url": "http://adrianortega.dev/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrian-ortega/duster-brews-dashboard/issues"
  },
  "homepage": "https://github.com/adrian-ortega/duster-brews-dashboard#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-es6-template-engine": "^2.2.3",
    "formidable": "^2.1.1",
    "googleapis": "^39.2.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5"
  }
}
